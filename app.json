{
  "expo": {
    "name": "Dica",
    "slug": "dica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-300x200.png",
    "scheme": "dica",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-300x200.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arodrigues.tks.dica",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-300x200.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arodrigues.tks.dica",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-300x200.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-300x200.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "86fbcd4a-b5ee-4c3c-a20b-095d2f0bf11c"
      }
    },
    "owner": "arodrigues.tks"
  }
}
